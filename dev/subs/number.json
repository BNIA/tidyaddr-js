{
    "ignore":[],
    "sub":[
        {"match":"^\\s*[0-9]+\\b","replace":null,"opts": {"flags":"gi"}},
        {"match":"^\\s*[0-9]+-[0-9]+\\b","replace":null,"opts": {"flags":"gi"}},
        {"match":"^\\s*[0-9]+\\.5\\b","replace":null,"opts": {"flags":"gi"}},
        {"match":"^\\s*[0-9]+[a-z]\\b","replace":null,"opts": {"flags":"gi"}},
        {"match":"^\\s*[0-9]+\\b1/2\\b","replace":null,"opts": {"flags":"gi"}},
        {"match":"^\\s*1/2\\b","replace":null,"opts": {"flags":"gi"}}
    ],
    "replace_only":
    [
            {"match":"^\\s*0*([1-9][0-9]*)\\b","replace":"$1","opts": {"flags":"gi"}},
        {"match":"^\\s*0*([1-9][0-9]*\\.5)\\b","replace":"$1","opts": {"flags":"gi"}},
        {"match":"^\\s*0*([1-9][0-9]*[a-z])\\b","replace":"$1","opts": {"flags":"gi"}},
        {"match":"^\\s*0*([1-9][0-9]*\\b1/2)\\b","replace":"$1","opts": {"flags":"gi"}},
        {"match":"^\\s*([0-9]*)\\.5\\b","replace":"$1 1/2","opts":{"flags":"gi"}}
        ]
}
